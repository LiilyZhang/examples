{
    "org": "$HZN_ORG_ID",
    "label": "$SERVICE_NAME for $ARCH",
    "description": "zhangl - A super-simple sample Horizon service",
    "public": true,
    "documentation": "",
    "url": "$SERVICE_NAME",
    "version": "$SERVICE_VERSION",
    "arch": "$ARCH",
    "sharable": "singleton",
    "matchHardware": null,
    "requiredServices": [
    ],
    "userInput": [],
    "deployment": {
        "services": {
            "$SERVICE_NAME": {
                "image": "${DOCKER_IMAGE_BASE}_${ARCH}:${SERVICE_VERSION}",
                "privileged": true,
                "ports": [
                    {
                        "HostPort": "1883:1883/tcp",
                        "HostIP": "0.0.0.0"
                    }
                ]
            }
        }
    },
    "deploymentSignature": "",
    "imageStore": null
}